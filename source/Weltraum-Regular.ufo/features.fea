# Prefix: Languagesystems
# automatic
languagesystem DFLT dflt;

feature aalt {
# automatic
feature calt;
feature ss01;
feature ss02;
} aalt;

feature calt {
sub L A' by L_A.calt;
sub A' J by A_J.calt;
sub T R' by R.calt;
sub T P' by P.calt;
sub L B' by L_B.calt;
sub [T P R F] B' by F_B.calt;
sub F F' by F.calt;
sub T T' by T.calt;
} calt;

feature ss01 {
# automatic
sub C by C.ss01;
sub M by M.ss01;
sub N by N.ss01;
} ss01;

feature ss02 {
# automatic
sub M by M.ss02;
} ss02;

